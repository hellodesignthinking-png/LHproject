                {% for housing_type in housing_types %}
                <tr>
                    <td class="bold">{{ housing_type.name | default('N/A') }}</td>
                    <td class="number">{{ housing_type.score | default(0) | round(1) }}</td>
                    <td class="number">{{ housing_type.suitability | default(0) | round(0) }}%</td>
                    <td class="number">{{ housing_type.recommended_units | default(0) }}</td>
                </tr>
                {% else %}
                <tr>
                    <td colspan="4" class="text-center">No housing type data available</td>
                </tr>
                {% endfor %}
            </tbody>
        </table>
        
        <!-- v21 UPGRADE: Professional Demand Interpretation -->
        {% if demand_interpretation %}
            {{ demand_interpretation | safe }}
        {% endif %}
        
        <!-- v21 UPGRADE: Fallback for Missing Demand Data -->
        {% if demand_fallback %}
            {{ demand_fallback | safe }}
        {% endif %}
        
        <!-- v21 UPGRADE: Fallback for Missing Housing Type Data -->
        {% if housing_type_fallback %}
            {{ housing_type_fallback | safe }}
        {% endif %}
    </div>
</div>

